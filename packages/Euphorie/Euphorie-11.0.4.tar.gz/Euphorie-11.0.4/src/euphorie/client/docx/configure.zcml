<configure
    xmlns="http://namespaces.zope.org/zope"
    xmlns:browser="http://namespaces.zope.org/browser"
    i18n_domain="euphorie"
    >

  <browser:page
      name="oira-report.docx"
      for="euphorie.ghost.PathGhost"
      permission="zope2.View"
      class=".views.ActionPlanDocxView"
      layer="euphorie.client.interfaces.IClientSkinLayer"
      />

  <browser:page
      name="identification-report.docx"
      for="euphorie.ghost.PathGhost"
      permission="zope2.View"
      class=".views.IdentificationReportDocxView"
      layer="euphorie.client.interfaces.IClientSkinLayer"
      />

</configure>
