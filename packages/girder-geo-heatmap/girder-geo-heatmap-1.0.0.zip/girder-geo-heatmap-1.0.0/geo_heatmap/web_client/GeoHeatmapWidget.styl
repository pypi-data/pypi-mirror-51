@import "~nib/index.styl"

.g-geo-heatmap-header
  background-color #f0f0f0
  color #555
  font-size 16px
  font-weight bold
  padding 5px 6px
  margin-top 20px

.g-geo-heatmap-widget
  border 1px solid #f0f0f0
  width 100%
  height 600px
  -webkit-touch-callout none
  user-select none
  overflow hidden

  &:focus
    outline none
