<!-- subreport template for test_subreport -->
<report>
 <parameter name="title" default="''" />
 <font name="sbody" size="14" typeface="Arial" italic="true" />
 <layout pagesize="A6">
  <style color="#000000" font="sbody" />
  <header>
   <field expr="title" align="right" stretch="true" >
    <box x="10" height="1" width="-1cm" />
   </field>
   <line pen="dot">
    <box x="5" y="5" height="0" width="-5" float="true" />
   </line>
  </header>
  <footer>
   <box height="20" />
   <field expr="PAGE_NUMBER" align="right" format="%i /">
    <box x="7cm" width="2cm" />
   </field>
   <field expr="PAGE_NUMBER" evaltime="report" align="left">
    <box x="9cm" width="2cm" />
   </field>
  </footer>
  <detail>
   <field stretch="true">
    <data>sub-item</data>
    <box x="5cm" />
   </field>
   <field expr="item" stretch="true">
    <box x="7cm" />
   </field>
  </detail>
 </layout>
</report>
<!-- vim: set et ft=xml sw=1 : -->
