{
  "name": "@jupyterlab/zenodo",
  "version": "1.0.0",
  "description": "JupyterLab extension for uploading to Zenodo",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "zenodo"
  ],
  "homepage": "https://github.com/ChameleonCloud/jupyterlab-zenodo",
  "bugs": {
    "url": "https://github.com/ChameleonCloud/jupyterlab-zenodo/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChameleonCloud/jupyterlab-zenodo.git"
  },
  "license": "None",
  "author": "Maxine King",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js",
    "style/**/*.*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "prettier": "prettier --write '**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}'",
    "test": "make tests",
    "watch": "tsc -w"
  },
  "lint-staged": {
    "**/*{.ts,.tsx,.css,.json,.md}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@jupyterlab/application": "^1.0.0",
    "@jupyterlab/apputils": "*",
    "@jupyterlab/codeeditor": "*",
    "@jupyterlab/coreutils": "*",
    "@jupyterlab/docmanager": "*",
    "@jupyterlab/docregistry": "*",
    "@jupyterlab/filebrowser": "*",
    "@jupyterlab/fileeditor": "*",
    "@jupyterlab/mainmenu": "*",
    "@jupyterlab/services": "*",
    "@phosphor/coreutils": "*",
    "@phosphor/disposable": "*",
    "@phosphor/domutils": "*",
    "@phosphor/messaging": "*",
    "@phosphor/widgets": "*",
    "pdfjs-dist": "^2.0.943",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "husky": "^2.4.1",
    "lint-staged": "^8.2.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.18.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.5.2"
  },
  "resolutions": {
    "@types/react": "~16.4.13"
  },
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "managers": [
          "pip"
        ],
        "base": {
          "name": "jupyterlab_zenodo"
        }
      }
    }
  }
}
