{
    "python": {
        "official_name": "Python",
        "extensions": [ ".py" ],
        "comments": [ "^#.+$", "\"\"\"((?!\"\"\")[\\s\\S])*\"\"\"" ]
    },
    "c": {
        "official_name": "C",
        "extensions": [ ".c", ".h" ],
        "comments": ["^//.+$", "/\\*((?!\\*/)[\\s\\S])*\\*/" ]
    },
    "c++": {
        "official_name": "C++",
        "extensions": [ ".cpp", ".hpp", ".cc", ".hh" ],
        "comments": ["^//.+$", "/\\*((?!\\*/)[\\s\\S])*\\*/" ]
    },
    "java": {
        "official_name": "Java",
        "extensions": [ ".java" ],
        "comments": ["^//.+$", "/\\*((?!\\*/)[\\s\\S])*\\*/" ]
    },
    "javascript": {
        "official_name": "JavaScript",
        "extensions": [ ".js" ],
        "comments": ["^//.+$", "/\\*((?!\\*/)[\\s\\S])*\\*/" ]
    },
    "latex": {
        "official_name": "LaTeX",
        "extensions": [ ".tex" ],
        "comments": [ "^%.+$" ]
    },
    "vhdl": {
        "official_name": "VHDL",
        "extensions": [ ".vhd", ".vhdl" ],
        "comments": [ "^--.+$" ]
    }
}
